         break;
         case 7:
              crush(global.playership[i].siphontarget, 10, 0);
         break;
        }
       }
      }
      //Send to everyone
      bufferpack();
      client_sendall(0, 0);
 break;
 default:
    refreshing -= 1;
 break;   
 
