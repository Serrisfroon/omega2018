            countdown_yscaling[1] += 0.0625;
        if(countdown_yscaling[1] = 1)
        {
            countdown_xscaling[3] = 0.7;
            countdown_yscaling[3] = 0.7;
            countdown_alpha[3] = 0.6;
            animation_step += 1;
        }    
    break;
    case 3:
        if(countdown_xscaling[1] &gt; 1)
            countdown_xscaling[1] -= 0.4;
        else
            if(countdown_alpha[1] &gt; 0.3)
                countdown_alpha[1] -= 0.05;
        if(countdown_xscaling[3] &lt; 1.25)
        {
            countdown_xscaling[3] += 0.08;   
            countdown_yscaling[3] += 0.08;   
        }
        else
        {
            countdown_xscaling[3] = 1.25;
            countdown_yscaling[3] = 1.25;
            countdown_alpha[2] = 0.5;
            animation_step += 1;
            alarm[4] = 10;
        }
    break;
    case 4:
        countdown_alpha[2] += 0.1;
        countdown_alpha[3] -= 0.1;
        countdown_alpha[1] -= 0.05;
    break;
    case 5:
        countdown_xscaling[2] -= 0.005;
        countdown_yscaling[2] -= 0.005;
        if(countdown_xscaling[2] &lt;= 0.85)
        {
            countdown_alpha[2] -= 0.1;
            if(countdown_alpha[2] &lt;= 0)
            {
                alarm[4] = 2;
                for(i = 1; i &lt;= 3; i++)
                {
                    countdown_xscaling[i] = 1;
                    countdown_yscaling[i] = 1;
                    countdown_alpha[i] = 0;
                }
                animation_step = 0;
                countdown_second -= 1;
                if(countdown_second = 0)
                {
                    instance_create(0, 0, obj_begin_operation);
                    alarm[0] = 1;
                }
            }
        }
    break;
                

        
}
