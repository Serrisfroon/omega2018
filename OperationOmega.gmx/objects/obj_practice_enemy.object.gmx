  newdir = aim_limit(direction, point, 30);
  bullet_create(x+lengthdir_x(9, direction), y+lengthdir_y(9, direction), "Needle", newdir-3+random(6), 1, 0);
  shoot = 7;
 }
 else
  shoot -= 1;
}

