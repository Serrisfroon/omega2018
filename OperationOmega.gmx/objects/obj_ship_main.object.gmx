        writebyte(21, 0)//Treat pulse as a secondary
        writebyte(1, 0);
        //Send to everyone if host
        if(global.master = true)
            client_sendall(0, 0);
        else
            //Send to host if client
            sendmessage(global.host, 0, 0, 0);
    break;
}

