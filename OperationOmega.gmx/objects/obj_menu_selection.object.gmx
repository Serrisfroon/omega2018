            <kind>1</kind>
            <string>//Exit unless the mouse is over this selection
if(mouse_over_transition &gt; 0 || animation_step != 1 || animation_step = 4)
    exit;
if(global.sfxtoggle = 0)
    audio_play_sound(snd_clicktext, 1, 0);
switch(text_sprite)
{
    case spr_musicon:
        global.musictoggle = 1;
        text_sprite = spr_musicoff;
        exit;
    break;
    case spr_musicoff:
        global.musictoggle = 0;
        text_sprite = spr_musicon;
        exit;
    
    break;
    case spr_sfxon:
        global.sfxtoggle = 1;
        text_sprite = spr_sfxoff;
        exit;   
    break;
    case spr_sfxoff:
        global.sfxtoggle = 0;
        text_sprite = spr_sfxon;
        exit;       
    break;
    case spr_firing:
        global.firingmode = 1;
        text_sprite = spr_firing2;
        exit;   
    break;
    case spr_firing2:
        global.firingmode = 0;
        text_sprite = spr_firing;
        exit;       
    break;
    case spr_groupsettings:
        obj_public_mod.persistent = 1;
        obj_public_mod.cyber_window[0].persistent = 1;
    break;
    case spr_leave_group:
        obj_public_mod.persistent = 1;
        obj_public_mod.cyber_window[0].persistent = 1;    
        obj_public_mod.cyber_window[1].animation = 3;    
        obj_public_mod.cyber_window[2].animation = 3;    
        global.in_group = 0;
    break;
    case spr_back:
        if(global.in_group = 1)
        {
            global.in_group = 0;
            obj_waiting_boxes.persistent = 0;
        }
    break;
    case spr_create_group:
        global.in_group = 1;
        global.group_placement = 1;
    break;
}
//Fade out all selections
with(obj_menu_selection)
{
    animation_step = 2;
}
//Move this selection to the center of the menu
animation_step = 3;
//Signal the menu box to close
obj_menu_box.animation = 6;
switch(room)
{
    case rm_main_menu:
        obj_profile_connect.animation = 3;
    break;
    case rm_public_game:
        if(text_sprite = spr_back)
            with(obj_waiting_boxes)
                animation = 3;
    break;
}
</string>
