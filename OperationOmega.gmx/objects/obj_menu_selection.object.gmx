        <arguments>
          <argument>
            <kind>1</kind>
            <string>switch(animation_step)
{
    case 0:
        if(image_index &gt; 13 &amp;&amp; text_alpha &lt; 1) 
            text_alpha += 0.05;    
        if(text_alpha = 1)
        {  
            animation_step += 1;
            if!(mouse_x &gt; x-104 &amp;&amp; mouse_x &lt; x+104 &amp;&amp; mouse_y &gt; y-19 &amp;&amp; mouse_y &lt; y+19)
                mouse_over = 1;
        }
    break;
    case 1:    
        if(mouse_over = 1)
        {
            if(mouse_over_transition &lt; 1)
            {
                mouse_over_transition += 0.1;
                image_xscale -= 0.025;
                image_yscale -= 0.025;
            }
        }
        else
        {
            if(mouse_over_transition &gt; 0)
            {
                mouse_over_transition -= 0.1;
                image_xscale += 0.025;
                image_yscale += 0.025;
            }        
        }   
    break;
    case 2:
        image_alpha -= 0.1;
        text_alpha -= 0.1;
    break;
    case 3:
        if(y &gt; obj_menu_box.y+6)
            y -= 6;
        else
            if(y &lt; obj_menu_box.y-6)
                y += 6;
            else
            {
                y = obj_menu_box.y;
                animation_step = 4;

                switch(text_sprite)
                {
                    case spr_host:
                        //Create the popup
                        (instance_create(384, 240, obj_creategamebox)).selection_reference = id;                                
                        
                        //Set the default options for the popup
                        global.maxplayers = 4;
                        global.matchtype = "stock";
                        global.matchvariable = 3;
                    break;
                    case spr_connect:
                        //Create a popup
                        (instance_create(384, 240, obj_enterhostip)).selection_reference = id;
                    break;
                    case spr_quickplay:
                        (instance_create(384, 240, obj_publicgame_search)).selection_reference = id;
                    break;
                    //case spr_searchforgame:
                    //    (instance_create(384, 240, obj_searchforgame_box)).selection_reference = id;                        
                    //break;
                    case spr_join_group:
                        (instance_create(384, 240, obj_joingroup_box)).selection_reference = id;                                            
                    break;
                    case spr_leave_group:
                    ////////////////////////////////////////////////////////////////////////////////////////////
                    //Group leaving netcode
                    ///////////////////////////////////////////////////////////////////////////////////////////
                    default:
                        alarm[1] = 20;
                    break;
                }
            }
    break;
    case 5:
        image_xscale += 0.08;
        image_yscale -= 0.1;
        if(image_yscale &lt;= 0)
        {
           visible = 0;
           alarm[0] = 10;
           animation_step = 6;
        }  
    break;
    case 6:
        if(y &lt;= ystart+6)
        {
            y = ystart;
            animation_step = 1;
        }
        else
            y -= 6;
    break;
    case 7:
        if(y &lt; ystart)
        {
            if(y &gt;= ystart-6)
            {
                y = ystart;
                animation_step = 1;
            }
            else
                y += 6;
        }
        if(y &gt; ystart)
        {
            if(y &lt;= ystart+6)
            {
                y = ystart;
                animation_step = 1;
            }
            else
                y -= 6;
        }
    
    break;
}
</string>
