        draw_sprite_ext(sprite_index, 8, x, y, 1, 1, 0, c_white, usealpha);    
    else
    {    
        draw_sprite_ext(sprite_index, global.selected_ship[mid]+((global.team[mid]-1)*3)-1, x, y, 1, 1, 0, c_white, usealpha);
        if(global.readied[mid] = 2 || mid = 0)
