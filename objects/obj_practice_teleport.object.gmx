            <kind>1</kind>
            <string>global.object_view = obj_ship;
view_object = global.object_view;

//Set scales to 0, makes the object invisible
image_xscale = 0;
image_yscale = 0;

//Set the direction and rotate the image to reflect it
direction = point_direction(x, y, room_width/2, room_height/2);
image_angle = direction;

//Set the time until the animation begins
alarm[0] = 30;
go = 0;

//Wait a step until changing image
alarm[5] = 1;

//Stay at the first frame of the sprite
image_speed = 0;
image_index = 0;

//Set Wormhole values
xscale = 0;
yscale = 0;
alpha = 1;

//Animation variable
image = 0;

//Set variables for two ring effects
ring1 = 0;
ring1alpha = 1;
ring1xscale = 0;
ring1yscale = 0;
ring1x = x;
ring1y = y;
ring2 = 0;
ring2alpha = 1;
ring2xscale = 0;
ring2yscale = 0;
ring2x = x;
ring2y = y;

system = part_system_create();
//particles shooting
primarypart = part_type_create();
part_type_shape(primarypart,pt_shape_flare);
part_type_size(primarypart,0.13,0.13,0,0);
part_type_scale(primarypart,1,1);
part_type_color1(primarypart,c_white);
part_type_alpha3(primarypart,0.86,0.36,0.09);
part_type_speed(primarypart,1.68,2.80,-0.04,0);
part_type_direction(primarypart,0,359,0,0);
part_type_gravity(primarypart,0,270);
part_type_orientation(primarypart,0,0,0,0,1);
part_type_blend(primarypart,0);
part_type_life(primarypart,20,30);




//Error prevention
smokenow = 0;
//Set ability to shoot
shoot = 0;
//Set initial target values
target = -1;
targetx = 0;
targety = 0;
dusted = 0;
newdir = 0;
chained = 0;
smoke = 6;

//Set values of ship to avoid error
cooldown = 0;
energy = 100;
heat = 50;
shield = 100;
armor = 100;
maxshield = 100;
maxarmor = 100;
autoturn = 0;
dualmissile = 0;
hit = 0;
secondary = 0;
ismine = 0;
