        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Perform the current step
switch(go)
{
 case 1:
      //Grow the wormhole
      yscale += 0.1;
      xscale += 0.06;
      //Move to the next step and set times until the ring effects when fully grown
      if(yscale = 1)
      {
       go = 2;
       alarm[3] = 3;
       alarm[4] = 6;
      }
      break;
 case 2:
      //Make the ship appear and move it at a high speed
      speed = 20;
      if(image_xscale != 1)
      {
       image_xscale += 0.2;
       image_yscale += 0.1;
      }
 break;
 case 3:
      //Finish showing the ship
      image_yscale += 0.1;
      //When finished, set the time until the next step
      if(image_yscale = 1)
      {
       go = 4;
       alarm[2] = 30;
      }
 break;
 case 5:
      //Make the wormhole disappear
      yscale -= 0.1;
      xscale -= 0.06;
      alpha -= 0.09;
      //Create a spark when the wormhole disappears
      if(yscale = 0)
      {
       effect_create_below(ef_spark, xstart, ystart, 1, c_white);
       go = 6;
      }
 break;
}
//Animate the first ring when it is active
if(ring1 = 1)
{
 ring1xscale += 0.045;
 ring1yscale += 0.075;
 if(ring1yscale &gt;= 0.75)
  ring1alpha -= 0.08;
}
//Animate the Second ring when it is active
if(ring2 = 1)
{
 ring2xscale += 0.03;
 ring2yscale += 0.05;
 if(ring2yscale &gt;= 0.5)
  ring2alpha -= 0.08;
}
//Animation Variable
image += 1;
</string>
          </argument>
        </arguments>
      </action>
