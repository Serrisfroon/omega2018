maxdstock[2] = 1;
maxdstock[3] = 1;
controlling = false;
//Initialize Hacker Mine hack variable
hacked = false;
//Initialize Shock Missile shock variable
shocked = false;
facedir = direction;;
beserk = 0;
beserking= 0;
thisturn = 0;
predictturn = 0;
predictspeed = 0;
nowshoot = 0;
mid = global.mymid;
//Set the correct colors to be displayed on radar
